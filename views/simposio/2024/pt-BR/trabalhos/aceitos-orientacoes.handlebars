\copyrightyear{2024}
\acmYear{2024}
\setcopyright{sbcpt} %%% Para artigos escritos em inglês, troque para sbcen
\acmConference[EduComp'24]{Simpósio Brasileiro de Educação em
Computação}{Abril 22-27, 2024}{São Paulo, São Paulo, Brasil (On-line)}
\acmBooktitle{Simpósio Brasileiro de Educação em Computação (EduComp
'24), Abril 22-27, 2024, São Paulo, São Paulo, Brasil (On-line)}

                </pre>

                <h4 class="mt-5 mb-3">Modelo Word:</h4>
                <p class="lead">

