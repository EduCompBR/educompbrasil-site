<!DOCTYPE html>
<html lang="pt-BR">
  
  {{~#if isEducomp}}
    {{> simposio/2025/educomp/pt-BR/head}}
  {{~else}}
    {{~#if isSbceb}}
      {{> simposio/2025/sbc-eb/pt-BR/head}}
    {{~/if}}
  {{~/if}}

  <body>
 
    <header class="header {{~#if isEducomp}} header-educomp {{~else}}{{~#if isSbceb}} header-sbc-eb {{~/if}}{{~/if}}">
      {{~#if isEducomp}}
        {{> simposio/2025/educomp/pt-BR/navbar}}
      {{~else}}
        {{~#if isSbceb}}
          {{> simposio/2025/sbc-eb/pt-BR/navbar}}
        {{~/if}}
      {{~/if}}
      
      {{~#if header.titulo }}
        <div class="d-flex justify-content-center align-items-center page-title">
          <h1>{{ header.titulo }}</h1>
        </div>
      {{~/if}}
    </header>

    <main class="content {{~#if isEducomp}} content-educomp {{~else}}{{~#if isSbceb}} content-sbc-eb {{~/if}}{{~/if}}">
      {{{ body }}}
    </main>

    {{~#if isEducomp}}
      {{> simposio/2025/educomp/pt-BR/footer}}
    {{~else}}
      {{~#if isSbceb}}
        {{> simposio/2025/sbc-eb/pt-BR/footer}}
      {{~/if}}
    {{~/if}}

    <!-- Bootstrap core JavaScript -->
    <script src="/vendor/jquery/jquery.min.js" type="text/javascript"></script>
    <script src="/vendor/bootstrap-4.3.1/js/bootstrap.bundle.min.js" type="text/javascript"></script>

    <!-- Plugin JavaScript -->
    <script src="/vendor/jquery-easing/jquery.easing.min.js" type="text/javascript"></script>

    <!-- Pesquisa e filtros -->
    <script src="/js/jquery.hideseek.js" type="text/javascript"></script>
    <script>
      $(document).ready(function () {
        $("#search").hideseek({
          nodata: "Nenhum item encontrado!",
        })
      })
    </script>

  </body>
</html>
